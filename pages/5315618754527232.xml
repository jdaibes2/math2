er.getModule('Text_Hint');\n  hint.setText('Add the digits in the tens place. Do not forget the ten you carry up. 3 + 2 + 1 = 6.');\n  hint.show();\n\nSCRIPTEND\nEVENTEND\n\nEVENTSTART\nSource: WritingCalculations5\nScore: 0\nSCRIPTSTART\n\n  var hint = presenter.playerController.getModule('Text_Hint');\n  hint.setText('Add the digits in the tens place: 8 + 5 + 1 = 14, you carry up one hundred and write 4 in the tens place.');\n  hint.show();\n\nSCRIPTEND\nEVENTEND\n\nEVENTSTART\nSource: WritingCalculations*\nScore: 1\nSCRIPTSTART\n\n  var hint = presenter.playerController.getModule('Text_Hint');\n  hint.hide();\n\nSCRIPTEND\nEVENTEND\n\n\nEVENTSTART\nSource: Submit1\nName: State\nSCRIPTSTART\n\n\n  var calc1 = presenter.playerController.getModule('WritingCalculations1');\n  var calc2 = presenter.playerController.getModule('WritingCalculations2');\n  var calc3 = presenter.playerController.getModule('WritingCalculations3');\n  var calc4 = presenter.playerController.getModule('WritingCalculations4');\n  var calc5 = presenter.playerController.getModule('WritingCalculations5');\n  \n  var h = presenter.playerController.getModule('Text_Hint');\n  var c = presenter.playerController.getModule('Text_Conclusion');\n  var g = presenter.playerController.getModule('GoToPage_Assessment3');\n\n  h.hide();\n\n  /* display conclusion */\n  if ($('#Check_Counter1').html() == '1') {\n    if (!calc1.isAllOK() || !calc2.isAllOK() || !calc3.isAllOK() || !calc4.isAllOK() || !calc5.isAllOK()) {\n      c.hide();\n      g.hide();\n    } else {\n      c.show();\n      g.show();\n    }    \n  } else {\n    c.hide();\n    g.hide();\n  }\n\n\nSCRIPTEND\nEVENTEND</property><property name="Is disabled" displayName="Is Disabled" type="boolean" value=""/></properties></addonModule><imageModule id="Image1" isTabindexEnabled="false"><layouts isVisible="true"><layout isLocked="false" isModuleVisibleInEditor="true" id="default"><relative type="LTWH"><left relative="" property="left"/><top relative="" p