CRIPTSTART\n\n  var hint = presenter.playerController.getModule('Text_Hint');\n\n  var txt = presenter.playerController.getModule('Text1');\n\n  if (!txt.isAllOK()) {\n    hint.setText("After 5 years, Nada's age will be: 12 + 5 = 17");\n    hint.show();\n  } else {\n    hint.hide();\n  }\n\nSCRIPTEND\nEVENTEND</property><property name="Is disabled" displayName="Is Disabled" type="boolean" value=""/></properties></addonModule></modules><groups></groups><editorRulers><vertical>0</vertical></editorRulers><page-weight value='1' mode='defaultWeight'></page-weight></page>