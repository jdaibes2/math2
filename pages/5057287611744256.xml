resenter.playerController.getModule('Audio1');\n  audio.play();\n\nSCRIPTEND\nEVENTEND\n\nEVENTSTART\nSource: Audio1\nName: ValueChanged\nValue: playing\nSCRIPTSTART\n\n  var audio = presenter.playerController.getModule('Audio2');\n  audio.stop();\n\nSCRIPTEND\nEVENTEND\n\nEVENTSTART\nSource: Audio2\nName: ValueChanged\nValue: playing\nSCRIPTSTART\n\n  var audio = presenter.playerController.getModule('Audio1');\n  audio.stop();\n\nSCRIPTEND\nEVENTEND\n\n\nEVENTSTART\nSource: Single_State_Button_*\nSCRIPTSTART\n\n  var vs = presenter.playerController.getModule('Variable_Storage1');\n  var num1 = 178;\n  var num2 = parseInt(vs.getVariable('number'));\n  var prev_call_time = parseFloat(vs.getVariable('call_time'));\n\n  var dt = new Date();\n  var current_call_time = dt.getTime(); \n\n  if (current_call_time - prev_call_time &gt; 100) {\n\n    if (event.source == 'Single_State_Button_Ones_Plus') {\n      num2 = num2 + 1;\n    } \n  \n    if (event.source == 'Single_State_Button_Ones_Minus') {\n      num2 = num2 - 1;\n    } \n\n    if (event.source == 'Single_State_Button_Tens_Plus') {\n      num2 = num2 + 10;\n    } \n  \n    if (event.source == 'Single_State_Button_Tens_Minus') {\n      num2 = num2 - 10;\n    } \n\n    vs.setVariable('number', num2);\n\n    var num1_ones = (num1 % 100) % 10;\n    var num1_tens = parseInt((num1 % 100) / 10);\n    var num2_ones = num2 % 10;\n    var num2_tens = parseInt(num2 / 10);\n\n    var txt_ones = presenter.playerController.getModule('Text_Ones');\n    txt_ones.setText(num2_ones);\n\n    var txt_tens = presenter.playerController.getModule('Text_Tens');\n    txt_tens.setText(num2_tens);\n\n    var sum = num1 + num2;\n    var sum_ones = (sum % 100) % 10;\n    var sum_tens = parseInt((sum % 100) / 10);\n    var sum_hundreds = parseInt(sum / 100);\n\n    var txt_sum_ones = presenter.playerController.getModule('Text_Sum_Ones');\n    txt_sum_ones.setText(sum_ones);\n\n    var txt_sum_tens = presenter.playerController.getModule('Text_Sum_Tens');\n    txt_sum_tens.setText(sum_tens);\n\n    var txt_sum_hundreds = presenter.playerController.getModule('Text_Sum_Hundreds');\n    txt_sum_hundreds.setText(sum_hundreds);\n\n    var txt_grouping_tens = presenter.playerController.getModule('Text_Grouping_Tens');\n    var txt_grouping_hundreds = presenter.playerController.getModule('Text_Grouping_Hundreds');\n\n    if (num1_ones + num2_ones &gt;= 10) {\n      txt_grouping_tens.show();\n\n      if (num1_tens + num2_tens + 1 &gt;= 10) {\n        txt_grouping_hundreds.show();\n      } else {\n        txt_grouping_hundreds.hide();\n      }\n    } else {\n      txt_grouping_tens.hide();\n\n      if (num1_tens + num2_tens &gt;= 10) {\n        txt_grouping_hundreds.show();\n      } else {\n        txt_grouping_hundreds.hide();\n      }\n    }\n\n    var ssb_ones_minus = presenter.playerController.getModule('Single_State_Button_Ones_Minus');\n\n    if (num2_ones == 0) {\n      ssb_ones_minus.disable();\n    } else {\n      ssb_ones_minus.enable();\n    }\n\n    var ssb_ones_plus = presenter.playerController.getModule('Single_State_Button_Ones_Plus');\n\n    if (num2_ones == 9) {\n      ssb_ones_plus.disable();\n    } else {\n      ssb_ones_plus.enable();\n    }\n\n    var ssb_tens_minus = presenter.playerController.getModule('Single_State_Button_Tens_Minus');\n\n    if (num2_tens == 0) {\n      ssb_tens_minus.disable();\n    } else {\n      ssb_tens_minus.enable();\n    }\n\n    var ssb_tens_plus = presenter.playerController.getModule('Single_State_Button_Tens_Plus');\n\n    if (num2_tens == 9) {\n      ssb_tens_plus.disable();\n    } else {\n      ssb_tens_plus.enable();\n    }\n  }\n\n  vs.setVariable('call_time', current_call_time);\n\nSCRIPTEND\nEVENTEND</property><property name="Is disabled" displayName="Is Disabled" type="boolean" value=""/></properties></addonModule><addonModule addonId="Variable_Storage" id="Variable_Storage1" isTabindexEnabled="false"><layouts isVisible="true"><layout isLocked="false" isModu